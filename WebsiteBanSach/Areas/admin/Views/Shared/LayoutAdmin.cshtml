<body>
    <!-- start: Header -->
    <div class="navbar">
        <div class="navbar-inner">
            <div class="container-fluid">
                
                <a class="brand" href="~/admin/SACHes"><span>Books</span></a>

                <!-- start: Header Menu -->
                <div class="nav-no-collapse header-nav">
                    <ul class="nav pull-right">
                       
                        <!-- start: User Dropdown -->
                        <li class="dropdown">
                            <a class="btn dropdown-toggle" data-toggle="dropdown" href="#">
                                <i class="halflings-icon white user"></i> Trần Đại Nghĩa
                                
                            </a>
                            
                        </li>
                        <!-- end: User Dropdown -->
                    </ul>
                </div>
                <!-- end: Header Menu -->

            </div>
        </div>
    </div>
    <!-- start: Header -->

    <div class="container-fluid-full">
        <div class="row-fluid">

            <!-- start: Main Menu -->
            <div id="sidebar-left" class="span2">
                <div class="nav-collapse sidebar-nav">
                    <ul class="nav nav-tabs nav-stacked main-menu">
                        <li><a href="@Url.Action("Index","SACHes")"><i class="fa-solid fa-book"></i></i><span class="hidden-tablet"> Quản lý Sản Phẩm</span></a></li>
                        <li><a href="@Url.Action("Index","KHACHHANGs")"><i class="icon-envelope"></i><span class="hidden-tablet"> Quản lý Khách Hàng</span></a></li>
                        <li><a href="@Url.Action("Index","DONHANGs")"><i class="icon-tasks"></i><span class="hidden-tablet"> Quản lý Đơn Hàng</span></a></li>
                        <li><a href="@Url.Action("Index","CHITIETDONHANGs")"><i class="icon-eye-open"></i><span class="hidden-tablet"> Chi tiết Đơn Hàng</span></a></li>
                        <li><a href="@Url.Action("Index","TACGIAs")"><i class="icon-dashboard"></i><span class="hidden-tablet"> Quản lý Tác Giả</span></a></li>
                        <li><a href="@Url.Action("Index","NXBs")"><i class="icon-edit"></i><span class="hidden-tablet"> Quản lý Nhà Xuất Bản</span></a></li>
                        <li><a href="@Url.Action("Index","THAMGIAs")"><i class="icon-list-alt"></i><span class="hidden-tablet"> Quản lý Tham Gia</span></a></li>
                        <li><a href="@Url.Action("Index","CHUDEs")"><i class="icon-font"></i><span class="hidden-tablet"> Quản lý Chủ Đề</span></a></li>
                        
                    </ul>
                </div>
            </div>
            <!-- end: Main Menu -->

            <noscript>
                <div class="alert alert-block span10">
                    <h4 class="alert-heading">Warning!</h4>
                    <p>You need to have <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a> enabled to use this site.</p>
                </div>
            </noscript>
         
            @* START CONTENT *@
            <div id="content" class="span10">
                @RenderBody()
            </div>
            @* END CONTENT *@

        </div><!--/#content.span10-->
    </div><!--/fluid-row-->

    

    <div class="clearfix"></div>

    <footer>

        <p>
            <span style="text-align:left;float:left">&copy; 2003 <a  alt="Bootstrap_Metro_Dashboard">Nghia~ and Huy</a></span>

        </p>

