    <link href="~/Areas/Bootstrap_Metro_Dashboard-master/css/bootstrap-responsive.min.css" rel="stylesheet" />
    <link href="~/Areas/Bootstrap_Metro_Dashboard-master/css/style.css" rel="stylesheet" />
    <link href="~/Areas/Bootstrap_Metro_Dashboard-master/css/style-responsive.css" rel="stylesheet" />

    <link href="~/Content/fontawesome-free-6.2.0-web/css/all.css" rel="stylesheet" />



